[Directories]
Root=
System=System
App=App
AppPublic=Public
[Server]
Port = 5000
MaxListeners=100
AccessLog=access.log
ErrorLog=error.log
[CPU]
Workers = 1
[MailSettings]
from=noreply@example.com
to=anybody@example.com
subject=From my site
[Session]
CookieMaxAge=900
CookieName=sf-session-id
CleanupInterval=5000
[HTTP]
ResponseDeflate=true
ResponseDeflateStaticFiles=true
ResponseDeflateFiletypes=txt,js,json,html,htm,xml,svg,css
StaticFilesCache=true
StaticFilesCacheMaxAge=31536000

